{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/index-file1-speed1x.html",
      "dest": "/index-file1-speed1x.html"
    },
    {
      "src": "/index-file2-speed2.5x.html",
      "dest": "/index-file2-speed2.5x.html"
    }
  ]
}

